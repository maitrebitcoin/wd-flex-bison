              width : 86
              height : 61
             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1+
                  
                  // nom de la classe a générer
                  // ex : "c:\temp\parseur_test_01.wdc"
                  fichier_dest		est chaîne	= nom_complet_fichier_généréré
                  si fichier_dest="" ALORS
                  	retour
                  FIN
                  
                  
                  quote est chaine = """"
                  // exe
                  ligne_de_commande est chaine = "wd-flex-bison.exe"
                  // les 2 paramètres obligatoire : .l .y .wdc
                  ligne_de_commande += " " + quote + nom_complet_fichier_lex      + quote
                  ligne_de_commande += " " + quote + nom_complet_fichier_bison    + quote
                  ligne_de_commande += " " + quote + nom_complet_fichier_généréré + quote
                  // option
                  SI InterrupteurModeVerbose  	 ALORS ligne_de_commande +=         LC_Verbose 
                  SI InterrupteurModeDebug    	 ALORS ligne_de_commande += [" "] + LC_Debug
                  SI InterrupteurProjeComplet  	 ALORS ligne_de_commande += [" "] + LC_CreeProject
                  SI InterrupteurComposantInterne  ALORS ligne_de_commande += [" "] + LC_CI_SiInexistant
                  
                  t est un toast
                  t.Titre = "Ligne de commande :"
                  t.Texte = ligne_de_commande
                  t.Bouton[1].Libellé = "Copier"
                  t.Bouton[1].ActionClic = ()=>{ VersPressePapier(ligne_de_commande) }
                  t.Bouton[2].Libellé = "Fermer"
                  t.CadrageVertical =chCentre
                  t.CouleurFond = noir
                  t.DuréeAffichage = 30s
                  t.Affiche()
