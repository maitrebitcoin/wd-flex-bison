     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      
      
      // sauver les paramètres de génération
      Sauve_pararamètre_compilation(nom_complet_fichier_lex)
     type : 2
  procedures :
   -
     name : _bExécuteFermeture
     procedure_id : 1418324597732122812
     type_code : 14
     code : |1-
      procédure _bExécuteFermeture()
      
      SI MaFenêtre..Modifié ALORS
      	SI PAS OuiNon(<§R13949b0c0206f527¤fr-FR¤Le projet a été modifié.CRVoulez-vous continuer et perdre vos modifications ?§>) ALORS
      		RENVOYER Faux
      	FIN
      FIN
      // OK
      RENVOYER Vrai
      
      // on crée un projet vide
     type : 458752
   -
     name : nouveau_projet
     procedure_id : 1418324597732188348
     type_code : 14
     code : |1-
      procédure nouveau_projet()
      
      repertoire_projet est chaine 
      ID est un entier = DonneIdentifiant()
      nom_projet  = "mon_compilateur" + ID
      procédure interne _genere_nom_fichier()
      
      	repertoire_projet = ComplèteRep(fRepDonnées())+nom_projet
      	nom_complet_fichier_lex   	 = ComplèteRep(repertoire_projet) + nom_projet + gsExt_lex
      	nom_complet_fichier_bison 	 = ComplèteRep(repertoire_projet) + nom_projet + gsExt_bison
      	nom_complet_fichier_généréré = ComplèteRep(repertoire_projet) + ComplèteRep(gsRepertoireGeneration) + nom_projet + gsExt_classeWD
      FIN
      _genere_nom_fichier()
      // si un des fichiers source existe déjà on génère un autre nom
      tantque fFichierExiste(nom_complet_fichier_lex) _ou_ fFichierExiste(nom_complet_fichier_bison) 
      	ID++
      	nom_projet  = "mon_compilateur" + ID
      	_genere_nom_fichier()
      fin
      // demande a l'utilisation
      //1 : OK
      //2 : Annuler
      SELON Saisie(<§?13CD739100606584§>,nom_projet) 
      	// OK
      	CAS 1
      		_genere_nom_fichier()
      	// Annuler
      	CAS 2
      		retour
      FIN
      
      
      // crée le répertoire du projet si il n'existe pas
      fRepCrée(repertoire_projet)
      // Init du contenu des fichier
      contenu_lex est un buffer = fChargeRessource("..\exemples\projet_vide.l")
      dbgAssertion(contenu_lex<>"")
      fSauveBuffer(nom_complet_fichier_lex,contenu_lex)
      contenu_bison est un buffer = fChargeRessource("..\exemples\projet_vide.y")
      dbgAssertion(contenu_bison<>"")
      fSauveBuffer(nom_complet_fichier_bison,contenu_bison)
      
      // init des libellé des volets avec les nom de fichier et des champs de saisie
      _GlobalesVersChamp()
      Table_Resultat.SupprimeTout()
      // gestion du dernier document ouvert
      on_charge_document(nom_complet_fichier_lex)
     type : 458752
   -
     name : _GlobalesVersChamp
     procedure_id : 1418324597732253884
     type_code : 14
     code : |1+
      procédure _GlobalesVersChamp()
      
      R_Compilation..Grisé = faux
      // init des libellé des volets avec les nom de fichier
      ONGLET_Source..Grisé= faux
      ONGLET_Source[1]..Libellé = "Mots (tokens) - " +  fExtraitChemin(nom_complet_fichier_lex,fFichier+fExtension)
      ONGLET_Source[2]..Libellé = "Grammaire - " + fExtraitChemin(nom_complet_fichier_bison,fFichier+fExtension)
      
      // Init des champ de saisie
      CodeSourceLexer     = charge_fichier_source(nom_complet_fichier_lex)
      CodeSourceGrammaire = charge_fichier_source(nom_complet_fichier_bison)
      SAI_Fichier_Dest	= nom_complet_fichier_généréré
     type : 458752
   -
     name : ouvre_projet_fSelectur
     procedure_id : 1418324597732319420
     type_code : 14
     code : |1+
      // ouvre un projet existant par ouverture du sélecteur de fichiers
      procédure ouvre_projet_fSelectur()
      
      // demander a l'utilisateur de choisir un fichier
      sFichier est chaîne = fSélecteur(fRepDonnées(), "","",gsFiltreLexBison, gsExt_lex, fselOuvre+fselExiste)
      SI sFichier = "" ALORS RETOUR
      // ouvrir le projet
      ouvre_projet_par_nom(sFichier, avecAjoutMRU )
      
      
     type : 458752
   -
     name : ouvre_projet_par_nom
     procedure_id : 1426817985771650335
     type_code : 14
     code : |1-
      // ouvre un projet t
      PROCÉDURE ouvre_projet_par_nom(sFichier, option est OPtionOuvre)
      
      // charger ce fichier
      repertoire_projet est chaine = fExtraitChemin(sFichier,fDisque+fRépertoire)
      nom_projet						= fExtraitChemin(sFichier, fFichier)
      repertoire_projet				= ComplèteRep(fRepDonnées())+nom_projet
      nom_complet_fichier_lex			= ComplèteRep(repertoire_projet) + nom_projet + gsExt_lex
      nom_complet_fichier_bison		= ComplèteRep(repertoire_projet) + nom_projet + gsExt_bison
      // RAZ du nom du fichier dest
      nom_complet_fichier_généréré	= ComplèteRep(repertoire_projet) + ComplèteRep(gsRepertoireGeneration) + nom_projet + gsExt_classeWD
      
      // recharger les paramètres de génération
      Charge_paramètres_compilation(nom_complet_fichier_lex)
      
      // init des libellé des volets avec les nom de fichier et des champs de saisie
      _GlobalesVersChamp()
      Table_Resultat.SupprimeTout()
      
      // gestion du dernier document ouvert
      si option= avecAjoutMRU alors
      	on_charge_document(sFichier) 
      fin
     type : 458752
   -
     name : enregistrer_sous_projet
     procedure_id : 1418324597732384956
     type_code : 14
     code : |1+
      procédure enregistrer_sous_projet()
      
      // demander a l'utilisateur de choisir un fichier
      sFichier est chaîne = fSélecteur(fRepDonnées(), "","",gsFiltreLexBison, gsExt_lex, fselCrée)
      SI sFichier = "" ALORS RETOUR
      //TODO
      
      // MAJ globale
      nom_projet = fExtraitChemin(sFichier,fFichier)
      repertoire_projet est chaine = fExtraitChemin(sFichier,fDisque+fRépertoire)
      nom_complet_fichier_lex			= ComplèteRep(repertoire_projet) + nom_projet + gsExt_lex
      nom_complet_fichier_bison		= ComplèteRep(repertoire_projet) + nom_projet + gsExt_bison
      nom_complet_fichier_généréré	= ComplèteRep(repertoire_projet) + ComplèteRep(gsRepertoireGeneration) + nom_projet + gsExt_classeWD 
      //sauver
      enregistrer_projet()
      
      // MAJ IHM
      _GlobalesVersChamp()
     type : 458752
   -
     name : enregistrer_projet
     procedure_id : 1418324597732450492
     type_code : 14
     code : |1-
      procédure enregistrer_projet()
      
      // sauver les paramètres de génération
      Sauve_pararamètre_compilation(nom_complet_fichier_lex)
      
      // sauver les fichier
      fSauveTexte(nom_complet_fichier_lex,CodeSourceLexer)
      si ErreurDétectée ALORS
      	erreur()
      FIN
      fSauveTexte(nom_complet_fichier_bison,CodeSourceGrammaire)
      SI ErreurDétectée ALORS
      	Erreur()
      FIN
      
      
      MaFenêtre..Modifié = faux
     type : 458752
   -
     name : compile_projet
     procedure_id : 1418324597732516028
     type_code : 14
     code : |1+
      procédure compile_projet()
      
      // sauver le paramètre de génération
      Sauve_pararamètre_compilation(nom_complet_fichier_lex)
      
      // nom de la classe a générer
      // ex : "c:\temp\parseur_test_01.wdc"
      fichier_dest		est chaine	= nom_complet_fichier_généréré
      fichier_dest_lex	est chaîne	= fExtraitChemin(fichier_dest,fDisque+fRépertoire+fFichier)+"_lex" + fExtraitChemin(fichier_dest,fExtension)
      // générer le répertoire destination si il n'existe pas
      repertoire_dest		est chaine	= fExtraitChemin(fichier_dest,fDisque+fRépertoire)
      fRepCrée(repertoire_dest)
      
      // Lancer la compilation
      compilateurBison  est un wdBison
      compilateurBison.en_mode_verbose	= InterrupteurModeVerbose
      compilateurBison.dest_en_mode_debug	= InterrupteurModeDebug
      compilateurBison.dest_projet_complet= InterrupteurProjeComplet
      compilateurBison.fichier_de_test    = SAI_Fichier_de_test
      ///b est un booléen
      //stErreur est wd_compilateur_base::ErreurCompile 
      soit (b,stErreur) = compilateurBison.génère_compilateur(nom_complet_fichier_lex,nom_complet_fichier_bison, fichier_dest)
      SI PAS b ALORS
      	//Erreur
      	_onCompile_Erreur(stErreur)
      	RETOUR
      	
      FIN
      // OK
      _onCompile_OK(fichier_dest)
      
     type : 458752
   -
     name : init_editeur_de_code
     procedure_id : 1421923986494916418
     type_code : 14
     code : |1+
      procédure init_editeur_de_code()
      // Init editeur de code .l et .y
      
      param_edc est Champ_editeur_de_code.ParametreEditeur
      param_edc.avec_numéro_de_ligne	= Vrai
      param_edc.avec_tab				= Vrai
      param_edc.nom_langage			= "js"
      CodeSourceLexer.init_editeur(param_edc)
      CodeSourceGrammaire.init_editeur(param_edc)
      
     type : 458752
   -
     name : _onCompile_OK
     procedure_id : 1422310640933711027
     type_code : 14
     code : |1-
      procédure _onCompile_OK(local fichier_dest est une chaîne)
      
      sMessage est chaine =  gRectangle(0,0,54,24,VertFoncé,VertClair) + gCoord(20,2) +  gStylo(Blanc) +"OK"
      sAction  est chaine =  gLienCliquable("ouvrir dans un navigateur", _onclicLien)
      procédure interne _onclicLien()
      	LanceAppliAssociée( fExtraitChemin(fichier_dest,fDisque+fRépertoire))
      fin
      Table_Resultat..Visible = vrai	
      TableSupprimeTout(Table_Resultat)
      TableAjouteLigne(Table_Resultat, nom_projet,sMessage, sAction)
      TableAjuste(Table_Resultat)
     type : 458752
   -
     name : _onCompile_Erreur
     procedure_id : 1422664258478143026
     type_code : 14
     code : |1+
      procédure _onCompile_Erreur( stErreur est wd_compilateur_base::ErreurCompile ) 
      
      sAction		est chaine
      fichier_source	est chaine    = stErreur.fichier_source
      ligne			est entier	  = stErreur.num_ligne
      message_dans_table est chaine =  stErreur.message_err
      
      // essie d'extraire le fichier source et le n° de ligne du message d'erreur
      info_source  est chaine
      
      si fichier_source<>"" alors
      	info_source = fExtraitChemin(fichier_source,fFichier+fExtension)+":"+ligne
      	sAction	=  gLienCliquable("aller sur l'erreur", _onclicLien)
      sinon
      	info_source = nom_projet
      fin
      // callbakc clic sur lien
      	PROCÉDURE INTERNE _onclicLien()
      		// mettre le curseur sur la ligne de l'erreur
      		bErreur_lex est un booléen = (fichier_source = nom_complet_fichier_lex)
      	 	si bErreur_lex ALORS
      	 		ONGLET_Source = 1 // aller sur le fichier lex
      	 		CodeSourceLexer.cmd_selectionne_une_ligne(ligne)
      	 		EcranPremier(CodeSourceLexer)
      	 	sinon
      	 		ONGLET_Source = 2 // aller sur le fichier bison
      	 		CodeSourceGrammaire.cmd_selectionne_une_ligne(ligne)
      	 		EcranPremier(CodeSourceGrammaire)
      	 	fin
      	 
      	FIN
      	
      sMessage	est chaine	=  gImage("erreur.svg") + " " + gStylo(RougeFoncé) + message_dans_table
      Table_Resultat..Visible = Vrai	
      TableSupprimeTout(Table_Resultat)
      TableAjouteLigne(Table_Resultat, info_source,sMessage, sAction)
      TableAjuste(Table_Resultat)
      beep()
      
      // découpe un emessage d'erreur en (fichier,n° de ligne,suite du message)
      procédure interne extraitFichierSourceEtLigneDeMessage(_message est chaine) :(chaine, entier,chaine)
      	// ex :
      	// source.l:33\nsyntax error
      	debut est chaine = ExtraitChaîne(_message,1,rc)
      	suite est chaine = milieu(_message,taille(debut)+taille(rc)+1)
      	sligne est chaine  = ExtraitChaîne(debut,1,":",DepuisFin)
      	si sligne=EOT renvoyer ("",0,"") // mauvais format
      	fichier est chaîne = gauche(_message, Taille(debut) - taille(sligne)-1)
      	si suite="" renvoyer ("",0,"") // mauvais format
      	nLigne est entier = sligne
      	// cas ou la ligne de code est vide
      	si ChaîneCommencePar(suite,rc) ALORS
      		// on l'enleve
      		suite = milieu(suite,taille(rc)+1)
      	FIN
      	// découpage ok
      	renvoyer (fichier,sligne,suite)
      fin
      
     type : 458752
   -
     name : Sauve_pararamètre_compilation
     procedure_id : 1429723213331946510
     type_code : 14
     code : |1+
      // sauver tous les paramètres utilisé pour la compilation
      procédure Sauve_pararamètre_compilation(fichier_lex_ou_bison)
      si fichier_lex_ou_bison="" ALORS
      	retour
      FIN
      nom_fichier_projet est chaine = fExtraitChemin(fichier_lex_ou_bison,fDisque+fRépertoire+fFichier)+gsExt_param
      si pas fFichierExiste(nom_fichier_projet) ALORS
      	fSauveTexte(nom_fichier_projet,"#28 fichier de paramètres pour wd-flex-bison"+rc)
      FIN
      
      INIEcrit("PARAM", "Destination", nom_complet_fichier_généréré, nom_fichier_projet)
      INIEcrit("PARAM", "Verbose",     InterrupteurModeVerbose,      nom_fichier_projet)
      INIEcrit("PARAM", "Debug",       InterrupteurModeDebug,        nom_fichier_projet)
      INIEcrit("PARAM", "Projet",      InterrupteurProjeComplet,     nom_fichier_projet)
      INIEcrit("PARAM", "FicTest",     SAI_Fichier_de_test,    	   nom_fichier_projet)
      INIEcrit("PARAM", "CI",      	 InterrupteurComposantInterne, nom_fichier_projet)
      INIEcrit("PARAM", "Onglet",      ONGLET_Source, 			   nom_fichier_projet)
     type : 458752
   -
     name : Charge_paramètres_compilation
     procedure_id : 1429727448171472360
     type_code : 14
     code : |1-
      // recharge les parmaètres sauvés par Sauve_pararamètre_compilation(=)
      procédure Charge_paramètres_compilation( fichier_lex_ou_bison )
      
      nom_fichier_projet est chaine = fExtraitChemin(fichier_lex_ou_bison,fDisque+fRépertoire+fFichier)+gsExt_param
      SI PAS fFichierExiste(nom_fichier_projet) ALORS
      	retour // pas de fichier de paramètres
      FIN
      fichier_Destination est chaine = INILit("PARAM", "Destination", "",   nom_fichier_projet)
      si fichier_Destination="" alors
      	RETOUR //  fichier de paramètres invalide
      FIN
      
      nom_complet_fichier_généréré = INILit("PARAM", "Destination", "",   nom_fichier_projet)
      dbgAssertion(fExtraitChemin(nom_complet_fichier_généréré,fExtension)<>gsExt_param)
      InterrupteurModeVerbose      = INILit("PARAM", "Verbose",     "0",  nom_fichier_projet)
      InterrupteurModeDebug		 = INILit("PARAM", "Debug",       "0",  nom_fichier_projet)
      InterrupteurProjeComplet     = INILit("PARAM", "Projet",      "0",  nom_fichier_projet)
      SAI_Fichier_de_test			 = INILit("PARAM", "FicTest",     "",   nom_fichier_projet)
      InterrupteurComposantInterne = INILit("PARAM", "CI",      	  "0",  nom_fichier_projet)
      SAI_Fichier_Dest = nom_complet_fichier_généréré
      ONGLET_Source				 = INILit("PARAM", "Onglet",  	  "1",  nom_fichier_projet)	
     type : 458752
