   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      global
      	nom_projet est chaine
      	nom_complet_fichier_lex   est une chaîne
      	nom_complet_fichier_bison est une chaîne
      	nom_complet_fichier_généréré est une chaîne
      	
      CONSTANT
      	gsExt_lex					= ".l"
      	gsExt_bison					= ".y"
      	gsExt_classeWD				= ".wdc"
      	gsExt_param					= ".prj_ly"
      	gsFiltreLexBison			= "source lex (*"+gsExt_lex +")"+TAB+"*" + gsExt_lex +  RC + ...
      								  "source bison (*"+gsExt_bison +")"+TAB+"*" + gsExt_bison +  RC + ...
      								  "Tous"  + TAB + "*.*"
      	gsRepertoireGeneration		= "out"
      	
      FIN
      OPtionOuvre est une énumération
      	avecAjoutMRU
      	sansAjoutMRU
      fin
   -
     code : |1+
      // on ne peux rien faire tant qu'on a pas ouvert ou créé un projet
      R_Compilation..Grisé = vrai
      ONGLET_Source..Grisé = vrai
      
      // Init de 2 champs éditeur du code .l et .y
      init_editeur_de_code()
      
      
      // ouvrir le projet ouvert si il existe encor
      dernier_projet est chaine = recup_dernier_document_charge()
      si dernier_projet<>"" et fFichierExiste(dernier_projet)
      	ouvre_projet_par_nom(dernier_projet, sansAjoutMRU )	
      fin
      
     type : 34
