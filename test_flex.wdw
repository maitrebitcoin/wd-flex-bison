             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1+
                  // sauver les fichiers
                  //fichier_source_l est chaine = nom_complet_fichier_lex
                  //fichier_source_y est chaine = nom_complet_fichier_bison
                  _Enregistrer()
                  
                  // nom de la classe a générer
                  // ex : "c:\temp\parseur_test_01.wdc"
                  fichier_dest est chaine = nom_complet_fichier_généréré
                  fichier_dest_lex est chaîne = fExtraitChemin(fichier_dest,fDisque+fRépertoire+fFichier)+"_lex" + fExtraitChemin(fichier_dest,fExtension)
                  // générer le répertoire dest si il n'exsite pas
                  repertoire_dest est chaine = fExtraitChemin(fichier_dest,fDisque+fRépertoire)
                  fRepCrée(repertoire_dest)
                  
                  
                  // Lancer la compilation
                  compilateurBison  est un wdBison
                  compilateurBison.en_mode_verbose	= InterrupteurModeVerbose
                  compilateurBison.dest_en_mode_debug = InterrupteurModeDebug
                  soit (b,sErr) = compilateurBison.génère_compilateur(nom_complet_fichier_lex,nom_complet_fichier_bison, fichier_dest)
                  si pas b ALORS
                  	Erreur( sErr )
                  	retour
                  FIN
                  // OK
                  t est un Toast
                  t.Titre = "Compilation OK"
                  t.Texte = ChaîneConstruit( <§R1394bc96024ff876¤fr-FR¤Fichiers générés :CR%1CR%2§>, fichier_dest, fichier_dest_lex)
                  t.CadrageVertical = cvBas
                  t.Bouton[1].Libellé = "Voir dans l'explorateur"
                  t.Bouton[1].ActionClic = ()=>{
                  	LanceAppliAssociée( fExtraitChemin(fichier_dest,fDisque+fRépertoire))
                  }
                  t.Affiche()
                  
                  
                  //ToastAffiche("Compilation OK" + rc + fichier_dest,toastCourt,cvHaut)
                  //RésultatDeLaCompilationLex   = UTF8VersAnsi( fChargeTexte(fichier_dest_lex) )
                  //RésultatDeLaCompilationBison = UTF8VersAnsi( fChargeTexte(fichier_dest) )
                  
