 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      global
      	nom_projet est chaine
      	nom_complet_fichier_lex   est une chaîne
      	nom_complet_fichier_bison est une chaîne
      	nom_complet_fichier_généréré est une chaîne
      	
   -
     type : 230
     enabled : false
  procedures :
   -
     name : _bExécuteFermeture
     procedure_id : 1410923020494951177
     type_code : 14
     code : |1-
      procédure _bExécuteFermeture()
      
      SI MaFenêtre..Modifié ALORS
      	SI PAS OuiNon(<§R13949b0c0206f527¤fr-FR¤Le projet a été modifié.CRVoulez-vous continuer et perdre vos modifications ?§>) ALORS
      		RENVOYER Faux
      	FIN
      FIN
      // OK
      RENVOYER Vrai
      
      // on crée un projet vide
     type : 458752
   -
     name : nouveau_projet
     procedure_id : 1410923256718666084
     type_code : 14
     code : |1-
      procédure nouveau_projet()
      
      repertoire_projet est chaine 
      ID est un entier = DonneIdentifiant()
      procédure interne _genere_nom_fichier()
      	nom_projet  = "mon_compilateur" + ID
      	repertoire_projet = ComplèteRep(fRepDonnées())+nom_projet
      	nom_complet_fichier_lex   	 = ComplèteRep(repertoire_projet) + nom_projet + ".l"
      	nom_complet_fichier_bison 	 = ComplèteRep(repertoire_projet) + nom_projet + ".y"
      	nom_complet_fichier_généréré = ComplèteRep(repertoire_projet) + "out\" + nom_projet + ".wdc"
      FIN
      _genere_nom_fichier()
      // si un des fichiers source existe déjà on génère un autre nom
      tantque fFichierExiste(nom_complet_fichier_lex) _ou_ fFichierExiste(nom_complet_fichier_bison) 
      	ID++
      	_genere_nom_fichier()
      fin
      // crée le répertoire du porejt si il n'existe pas
      fRepCrée(repertoire_projet)
      // Init du contenu des fichier
      contenu_lex est un buffer = fChargeRessource("..\exemples\projet_vide.l")
      dbgAssertion(contenu_lex<>"")
      fSauveBuffer(nom_complet_fichier_lex,contenu_lex)
      contenu_bison est un buffer = fChargeRessource("..\exemples\projet_vide.y")
      dbgAssertion(contenu_bison<>"")
      fSauveBuffer(nom_complet_fichier_bison,contenu_bison)
      
      
      // init des libellé des volets avec les nom de fichier
      ONGLET_Source..Grisé= faux
      ONGLET_Source[1]..Libellé =  fExtraitChemin(nom_complet_fichier_lex,fFichier+fExtension)
      ONGLET_Source[2]..Libellé =  fExtraitChemin(nom_complet_fichier_bison,fFichier+fExtension)
      
      // Init des champ de saisie
      CodeSourceLexer     = fChargeTexte(nom_complet_fichier_lex)
      CodeSourceGrammaire = fChargeTexte(nom_complet_fichier_bison)
      SAI_Fichier_Dest	= nom_complet_fichier_généréré
     type : 458752
   -
     name : _EnregistrerSous
     procedure_id : 1410928492285740116
     type_code : 14
     code : |1-
      procédure _EnregistrerSous()
      
      //TODO
     type : 458752
   -
     name : _Enregistrer
     procedure_id : 1410929089286792059
     type_code : 14
     code : |1-
      procédure _Enregistrer()
      
      // sauver les fichier
      fSauveTexte(nom_complet_fichier_lex,CodeSourceLexer)
      si ErreurDétectée ALORS
      	erreur()
      FIN
      fSauveTexte(nom_complet_fichier_bison,CodeSourceGrammaire)
      SI ErreurDétectée ALORS
      	Erreur()
      FIN
      MaFenêtre..Modifié = faux
     type : 458752
